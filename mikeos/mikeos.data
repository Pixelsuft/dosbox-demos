init:
  gosub startprg
  z = 9
  $5 = "Memory Manipulator"
  gosub settitle
  cls
  cursor off
  c = 7
  h = 9
  t = 1
  z = 1
  gosub refresh
  
  a = 5
  b = 4
  gosub update_screen
  gosub highlight_on
  
  x = ramstart
  y = progstart
  v = 0 - x
  x = x - y

