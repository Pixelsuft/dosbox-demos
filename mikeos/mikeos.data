  if q = 4 then print "Fire Laser     "
  if q = 5 then print "Mega Beam      "
  if q = 6 then print "Giga Beam      "
  if q > 6 then ink 71
  if q > 6 then print "Death Ray      "
  if q > 6 then ink 7
  ink 7
return

