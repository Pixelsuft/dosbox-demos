  gosub highlight_off
  b = b - 1
  a = 50
  d = d - 1
  gosub highlight_on
return

forward_line:
  if b = 19 then goto sforward_line
  gosub highlight_off
  b = b + 1
  a = 5
  d = d + 1
  gosub highlight_on
return

