update_screen:
  rem *** Cursor ***
  ink 14

  rem *** First, blank out area where cursor could be ***
  move 14 6
  print "                    "
  move 14 7
  print "                    "

  for a = 9 to 21
    move 11 a
    print "  "
  next a

  a = x + 14
  move a 6
  print "|" ;
  move a 7
  print "v" ;
  b = y + 9
  move 11 b
  print "->" ;
  ink 7


  rem *** Numbers ***
  a = 14
  b = 9

  m = RAMSTART

  for c = 1 to 81
    peek d m

    move a b
    if d > 0 then print d ;
    if d = 0 then print " " ;
    a = a + 2

    rem *** Handle wrapping ***
    if a = 32 then b = b + 1
    if a = 32 then a = 14
    if b = 12 then b = 13
    if b = 16 then b = 17

    m = m + 1
  next c
return


