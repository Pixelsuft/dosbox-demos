  gosub highlight_off
  a = 50
  f = f - 16
  d = d - 1
  gosub update_screen
  gosub highlight_on
return

sforward_line:
  gosub highlight_off
  a = 5
  f = f + 16
  d = d + 1
  gosub update_screen
  gosub highlight_on
return

page_up:
  gosub highlight_off
  f = f - 256
  d = d - 256
